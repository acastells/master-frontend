{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
	  "start": {
		"cache": false,
		"persistent": true,
		"dependsOn": ["build"]
	  },
	  "build": {
		"outputs": ["dist/**/*"],
		"dependsOn": ["^build", "type-check"]
	  },
	  "type-check": {
		"dependsOn": ["^build"]
	  },
	  "type-check:watch": {
		"dependsOn": ["build"]
	  }
	}
  }
  